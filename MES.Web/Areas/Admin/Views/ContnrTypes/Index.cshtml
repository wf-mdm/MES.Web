        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OVERLIMIT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CONTRLBY)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SUBCONTNRTYPE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PARENTTYPE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SINGLEPART)
        </td>
        <td>
            @Html.ActionLink("属性", String.Format("/{0}/Index", item.CONTNRTYPE), "ContnrAttrs", null, new { @class = "btn btn-success" })
            @Html.ActionLink("编辑", "Edit", new { id = item.CONTNRTYPE }, new { @class = "btn btn-info" })
            @Html.ActionLink("删除", "Delete", new { id = item.CONTNRTYPE }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
    </div>
</div>
