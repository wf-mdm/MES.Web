        <th>
            @Html.DisplayNameFor(model => model.CONTAINSUBQTY)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OVERLIMIT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CONTRLBY)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SUBCONTNRTYPE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PARENTTYPE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SINGLEPART)
        </th>
        <th width="280px">维护</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CONTNRTYPE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CONTAINERDESC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PARTGRP2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VOLUMEQTY)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VOL_UNIT)
        </td>
        <td>
